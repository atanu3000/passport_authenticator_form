<% if(locals.errors){ %> <% errors.forEach(function(error) { %>
<div class="flash text-center alert alert-danger" ><%= error.msg %></div>
<% }); %> <% } %> <% if(success_msg != ''){ %>
<div class="flash text-center alert alert-success" ><%= success_msg %></div>
<% } %> <% if(error_msg != ''){ %>
<div class="flash text-center alert alert-danger" ><%= error_msg %></div>
<% } %> <% if(error != ''){ %>
<div class="flash text-center alert alert-danger" ><%= error %></div>
<% } %>
